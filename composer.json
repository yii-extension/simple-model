{
    "name": "yii-extension/simple-model",
    "type": "library",
    "description": "Simple model for Yii3.",
    "keywords": [
        "yii",
        "simple",
        "model"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "yiisoft/validator": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.11",
        "vimeo/psalm": "^4.13"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Simple\\Model\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Simple\\Model\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
